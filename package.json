{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "npm run prisma:migrate:up && npm run prisma:generate && nest start",
    "start:watch": "npm run prisma:migrate:up && npm run prisma:generate && nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "prisma": "prisma",
    "prisma:db:create": "npm run prisma -- migrate dev",
    "prisma:migrate:up": "npm run prisma -- migrate deploy",
    "prisma:generate": "npm run prisma -- generate",
    "prisma:watch": "npm run prisma -- generate -- --watch",
    "prisma:silent-watch": "npm run prisma -- generate -- --watch &",
    "use-case": "node ./scripts/useCaseGenerator.mjs"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@apollo/subgraph": "^2.6.3",
    "@nestjs-modules/mailer": "^1.10.3",
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/common": "10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "10.3.0",
    "@nestjs/graphql": "12.0.11",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "10.3.0",
    "@prisma/client": "^5.8.1",
    "@prisma/sdk": "~4.0.0",
    "apollo-server-express": "3.13.0",
    "argon2": "^0.31.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "^1.4.6",
    "express-useragent": "^1.0.15",
    "graphql": "16.8.1",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.22.4",
    "graphql-subscriptions": "2.0.0",
    "graphql-type-json": "^0.3.2",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "nodemailer": "^6.9.8",
    "passport-custom": "^1.1.1",
    "prisma-extension-soft-delete": "^1.0.0",
    "reflect-metadata": "0.2.1",
    "rxjs": "7.1.0",
    "save-dev": "^0.0.1-security",
    "type-graphql": "^2.0.0-beta.6",
    "typegraphql-nestjs": "^0.6.0",
    "wrap-ansi": "8.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@clack/prompts": "^0.7.0",
    "@loancrate/prisma-schema-parser": "^2.0.0",
    "@nestjs/cli": "10.3.0",
    "@nestjs/schematics": "10.1.0",
    "@nestjs/testing": "10.3.0",
    "@prisma/generator-helper": "~5.7.1",
    "@types/express": "4.17.21",
    "@types/graphql-fields": "^1.3.3",
    "@types/node": "20.10.7",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "6.18.0",
    "@typescript-eslint/parser": "6.18.0",
    "dataloader": "^2.0.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "jest": "29.7.0",
    "prettier": "3.1.1",
    "prisma": "^5.8.1",
    "stringcase": "^4.3.1",
    "supertest": "6.1.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.2.2",
    "ts-node": "10.0.0",
    "tsconfig-paths": "4.2.0",
    "typegraphql-prisma": "^0.27.1",
    "typescript": "5.3.3",
    "zod-prisma": "^0.5.4"
  }
}
